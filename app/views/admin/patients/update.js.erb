$(".patients").html("<%= escape_javascript(render partial: 'patients', locals: {patients: @patients, title: 'Create Patient'}) %>")
<%- if @patient.errors.any?%>
  $(".phone-error").html("Vui Long nhap so dien thoai");
  $('#newPatient').modal('show');
<%- else%>
  $('#newPatient').on('hidden', function(){
    $(this).data('modal', null);
  });
<%- end%>